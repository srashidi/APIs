<%= form_for @kitten do |f| %>
	<%= render 'shared/error_messages', object: f.object %>

	<%= f.label :name %>
	<%= f.text_field :name %>
	<br><br>

	<%= f.label :age %>
	<%= f.number_field :age %>
	<br><br>

	<%= f.label :cuteness %>
	<%= f.text_field :cuteness %>
	<br><br>

	<%= f.label :softness %>
	<%= f.text_field :softness %>
	<br><br>

	<%= f.submit "Edit kitten" %>
<% end %>